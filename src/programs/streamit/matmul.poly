zeros out O[M][N] par i in 0:M-1 par j in 0:N-1 O[i,j] = 1 where M==N

matmul in first[M,N], second[M,N] out int32s result[M,N]
{
    par i in 0:M-1
      for j in 0:N-1 
        par k in 0:M-1 
          result[i,j] = result[i,j] + first[i,k] * second[k,j]
}where M==N


main
{
    var size = 10
    var a[size,size] = zeros()
    var result2[size,size] = matmul(a, a)
    int ret = extern print_array2 (size,size,a)
    ret = extern print_array2 (size,size,result2)
}
